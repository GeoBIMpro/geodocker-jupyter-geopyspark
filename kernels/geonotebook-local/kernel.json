{
 "language": "python",
 "display_name": "GeoNotebook + GeoPySpark (local)",
 "argv": [
  "/usr/bin/python3.4",
  "-m",
  "geonotebook",
  "-f",
  "{connection_file}"
 ],
  "env": {
      "LD_LIBRARY_PATH": "/home/hadoop/local/gdal/lib",
      "PYSPARK_PYTHON": "/usr/bin/python3.4",
      "SPARK_HOME": "/usr/local/spark-2.1.0-bin-hadoop2.7",
      "PYTHONPATH": "/usr/local/spark-2.1.0-bin-hadoop2.7/python/lib/pyspark.zip:/usr/local/spark-2.1.0-bin-hadoop2.7/python/lib/py4j-0.10.4-src.zip",
      "PYSPARK_SUBMIT_ARGS": "--master local[*] --driver-memory 8G --jars /blobs/geotrellis-backend-assembly-0.1.0.jar,/blobs/gddp-assembly-0.1.0.jar --conf spark.serializer=org.apache.spark.serializer.KryoSerializer --conf spark.kyro.registrator=geotrellis.spark.io.kyro.KryoRegistrator --conf spark.executorEnv.LD_LIBRARY_PATH=/home/hadoop/local/gdal/lib pyspark-shell"
  }
}
